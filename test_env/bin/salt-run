__requires__ = 'salt===2016.11.0-1181-ge23cc25'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('salt===2016.11.0-1181-ge23cc25', 'console_scripts', 'salt-run')()
    )
